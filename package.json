{
  "name": "starter-react",
  "version": "0.0.0",
  "packageManager": "pnpm@7.16.1",
  "description": "react template",
  "author": "starknt",
  "license": "MIT",
  "homepage": "https://github.com/starknt/starter-react#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/starknt/starter-react.git"
  },
  "keywords": [
    "react",
    "ts",
    "typescript"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "typecheck": "npx tsc --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest",
    "prepare": "npx simple-git-hooks"
  },
  "dependencies": {
    "@unocss/reset": "^0.46.5",
    "mobx": "^6.7.0",
    "mobx-react-lite": "^3.4.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "unocss": "^0.46.5"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.31.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/react-hooks": "^8.0.1",
    "@types/node": "^18.11.9",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.9",
    "@unocss/preset-attributify": "^0.46.5",
    "@vitejs/plugin-react": "^2.2.0",
    "eslint": "^8.27.0",
    "jsdom": "^20.0.2",
    "lint-staged": "^13.0.3",
    "pnpm": "^7.16.1",
    "react-test-renderer": "^18.2.0",
    "simple-git-hooks": "^2.8.1",
    "typescript": "^4.9.3",
    "unplugin-auto-import": "^0.11.4",
    "vite": "^3.2.4",
    "vitest": "^0.25.2"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "git add"
    ],
    "*.{json}": [
      "eslint --fix",
      "git add"
    ]
  }
}
